package chap8;

abstract class Animal{
	String name;
	Animal(String name){
		this.name = name;
	}
	abstract void eat();
}

interface Flyable{
	void fly();
}

class Dove extends Animal implements Flyable{
	String Dove;
	Dove() {
		super("비둘기");	
	}

	@Override
	public void fly() {
		System.out.println();
		
	}

	@Override
	void eat() {
		// TODO Auto-generated method stub
	}
	
	public String toString() {
		return "비둘기";
	}
}

class Monkey extends Animal{

	Monkey() {
		super("원숭이");
	}

	@Override
	void eat() {
		
	}
	
	public String toString() {
		return "원숭이";
	}
	
}

public class Exam1 {

	public static void main(String[] args) {
		Animal a = new Dove();
		a.eat();
		if(a instanceof Flyable) {
			((Flyable) a).fly();
		}
		a = new Monkey();
		a.eat();
		if(a instanceof Flyable) {
			((Flyable) a).fly();
		}
		

	}

}
