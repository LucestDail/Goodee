package Exam;

import java.util.Scanner;

public class Exam1 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		boolean end = true;
		while(end) {
			try {
				int target = scan.nextInt();
				if(target == 99) {
					end = !end;
					break;
				}
				for(int i = 0; i < target; i++) {
					System.out.print("*");
				}
				System.out.println();
				
				
			}catch(Exception e) {
				System.out.println("숫자만 입력하세요.");
				int target = scan.nextInt();
			}
		}
		
		

	}
}
