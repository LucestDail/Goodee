package chap11;

import java.util.Arrays;

public class ArrayEx1 {
	public static void main(String[] args) {
		String[] arr1 = {"¿ù¸Å","ÀÌ¸ù·æ","Çâ´Ü","¹æÀÚ"};
		for(String s : arr1) {
			System.out.print(s + ",");
		}
		
		System.out.println();
		Arrays.fill(arr1, "¼ºÃáÇâ");
		for(String s : arr1)
			System.out.print(s + ",");
		System.out.println();
		
		Arrays.fill(arr1, 1,3,"ÃáÇâÀü");
		for(String s : arr1)
			System.out.print(s + ",");
		System.out.println();
		
		int[] intArr = new int[10];
		for(int i : intArr)
			System.out.print(i + ",");
		System.out.println();
		
		String[] arr2 = {"¿ù¸Å","ÀÌ¸ù·æ","Çâ´Ü","¹æÀÚ"};
	}

}
