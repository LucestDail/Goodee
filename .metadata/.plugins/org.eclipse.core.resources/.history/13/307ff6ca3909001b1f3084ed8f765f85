package Test;

public class Test6 {
    public static void main(String[] args) {
        String src = "12345";
        System.out.println(fillZero(src, 10));
        System.out.println(fillZero(src, -1));
        System.out.println(fillZero(src, 3));
        System.out.println(fillZero(null, 3));
    }
}
