package chap8;

interface Printerable{
	int INK = 100;
	void print();
}

interface Scannerable{
	void scan();
}

interface Faxable{
	String FAX_NO = "02-1234-5678";
	void send(String tel);
	void receive(String tel);
}

interface Complexerable extends Printerable, Scannerable, Faxable{

}

