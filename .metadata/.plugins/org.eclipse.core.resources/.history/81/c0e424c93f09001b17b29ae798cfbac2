package Test1;

import java.util.Random;

public class Test5 {
    public static void main(String[] args) {
          for(int i=0;i<20;i++) {
          System.out.print(getRand(1,-3)+",");
          }
         System.out.println();
         for(int i=0;i<20;i++) {
             System.out.print(getRand(-1,3)+",");
         }
   }

	private static int getRand(int i, int j) {
		Random rand = new Random();
		rand.setSeed(System.currentTimeMillis());
		return rand.nextInt();
	}
}