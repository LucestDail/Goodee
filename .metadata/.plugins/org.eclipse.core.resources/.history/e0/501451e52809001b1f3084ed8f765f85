package chap11;

import java.util.Random;

public class RandomEx1 {
	public static void main(String[] args) {
		Random rand = new Random();
		rand.setSeed(1);;
		Random rand2 = new Random();
		rand2.setSeed(1);
		System.out.println("rand ===>");
		for(int i = 0; i < 5; i++) {
			System.out.println(i + " : " + rand.nextInt(100));
		}
		
		
		
		
		
	}
}
