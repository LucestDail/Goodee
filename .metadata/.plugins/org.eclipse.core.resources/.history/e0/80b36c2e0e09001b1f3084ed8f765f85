package chap11;

public class MyTest {
	public static void main() {
		if(ifStringMethod("hello","hellosame"))System.out.println("same");
		
		String str = "hello";
		
		System.out.println(str.contains("h"));
		
		
		
		
		
		
	}
	private static boolean ifStringMethod(String basic, String compare) {
		if(basic.equals(compare))return true;
		else return false;
	}
}
